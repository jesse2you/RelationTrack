externalPort = 80

[[ports]]
localPort = 33399
externalPort = 3002

[[ports]]
localPort = 33465
externalPort = 3003

[[ports]]
localPort = 35239
externalPort = 4200

[[ports]]
localPort = 39985
externalPort = 3000

[[ports]]
localPort = 45531
externalPort = 3001

[env]
PORT = "5000"

[agent]
integrations = ["javascript_mem_db:1.0.0", "javascript_openai_ai_integrations:1.0.0"]
